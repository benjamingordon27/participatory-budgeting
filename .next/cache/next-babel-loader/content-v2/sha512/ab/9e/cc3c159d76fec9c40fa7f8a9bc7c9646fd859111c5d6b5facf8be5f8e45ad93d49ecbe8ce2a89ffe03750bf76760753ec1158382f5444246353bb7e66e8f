{"ast":null,"code":"import { translateCategory, translateCategoryText } from '../../util/translateCategory';\nexport var itemsPerYear = function itemsPerYear(budget) {\n  var budgetAsArray = budget;\n  budgetAsArray = budgetAsArray.filter(function (item) {\n    return !isNaN(item.vote_year);\n  });\n  var years = [];\n  var outputYearData = [];\n  var keys = Object.keys(budgetAsArray);\n  keys.forEach(function (key) {\n    years.push(budgetAsArray[key].vote_year);\n  });\n  var allYears = years.filter(function (v, i, a) {\n    return a.indexOf(v) === i;\n  }).sort(function (a, b) {\n    return a - b;\n  });\n  allYears.map(function (currYear) {\n    var filteredArray = budget.filter(function (curr) {\n      return curr.vote_year === currYear;\n    });\n    var count = filteredArray.length;\n    var sumCost = 0;\n    var sumVotes = 0;\n    filteredArray.map(function (item) {\n      sumCost = sumCost + Number(item.cost);\n      if (!isNaN(item.votes)) sumVotes = sumVotes + Number(item.votes);\n    });\n    outputYearData.push({\n      title: \"\".concat(currYear),\n      count: count,\n      cost: sumCost,\n      votes: sumVotes\n    });\n  });\n  console.log(outputYearData);\n  return outputYearData;\n};\nexport var topTenItemsSorted = function topTenItemsSorted(howMany, budget, variable) {\n  var budgetAsArray = budget;\n  budgetAsArray = budgetAsArray.filter(function (item) {\n    return !isNaN(item[variable]);\n  });\n  budgetAsArray.sort(function (a, b) {\n    return Number(a[variable]) < Number(b[variable]) ? 1 : -1;\n  });\n  budgetAsArray.splice(howMany, budgetAsArray.length - howMany); // let data = budgetAsArray.map(item => Number(item[this.props.variable]));\n\n  var data = budgetAsArray;\n  data.forEach(function (item) {\n    return item[variable] = Number(item[variable]);\n  });\n  return data;\n};\nexport var bottomTenItemsSorted = function bottomTenItemsSorted(howMany, budget, variable) {\n  var budgetAsArray = budget;\n  budgetAsArray = budgetAsArray.filter(function (item) {\n    return !isNaN(item[variable]);\n  });\n  budgetAsArray.sort(function (a, b) {\n    return Number(a[variable]) > Number(b[variable]) ? 1 : -1;\n  });\n  budgetAsArray.splice(howMany, budgetAsArray.length - howMany); // let data = budgetAsArray.map(item => Number(item[this.props.variable]));\n\n  var data = budgetAsArray;\n  data.forEach(function (item) {\n    return item[variable] = Number(item[variable]);\n  });\n  return data;\n};\nexport var itemsPerDistrict = function itemsPerDistrict(budget, councilMembers, howMany, variable) {\n  var budgetAsArray = budget;\n  var outputDistrictData = [];\n  var districts = [];\n  var keys = Object.keys(councilMembers);\n  keys.forEach(function (key) {\n    districts.push(councilMembers[key].district);\n  });\n  var allDistricts = districts.filter(function (v, i, a) {\n    return a.indexOf(v) === i;\n  }).sort(function (a, b) {\n    return a - b;\n  });\n  allDistricts.map(function (item) {\n    var currCouncilMember = '';\n    var filteredArray = councilMembers.filter(function (i) {\n      return item === i.district.match(/\\d+/)[0];\n    });\n    if (filteredArray.length > 0) var currCouncilMember = filteredArray[[0]].name;\n    var sumCount = 0;\n    var sumVotes = 0;\n    budgetAsArray.filter(function (i) {\n      return item === i.council_district.match(/\\d+/)[0];\n    }).map(function (item) {\n      sumCount = sumCount + Number(item.cost);\n      if (!isNaN(item.votes)) sumVotes = sumVotes + Number(item.votes);\n    });\n    outputDistrictData.push({\n      title: \"District \".concat(item, \": \").concat(currCouncilMember),\n      count: budgetAsArray.filter(function (i) {\n        return item === i.council_district.match(/\\d+/)[0];\n      }).length,\n      cost: sumCount,\n      votes: sumVotes\n    });\n  });\n  outputDistrictData.sort(function (a, b) {\n    return Number(a[variable]) < Number(b[variable]) ? 1 : -1;\n  });\n  outputDistrictData.splice(howMany, outputDistrictData.length - howMany); // let data = budgetAsArray.map(item => Number(item[this.props.variable]));\n\n  var data = outputDistrictData;\n  data.forEach(function (item) {\n    return item[variable] = Number(item[variable]);\n  });\n  return data;\n};\nexport var totalSum = function totalSum(budget, variable) {\n  var budgetAsArray = budget;\n  var sum = 0;\n  budgetAsArray.map(function (item) {\n    if (item[variable]) sum = sum + Number(item[variable]);\n  });\n  return sum;\n};\nexport var itemsPerCategory = function itemsPerCategory(budget, howMany, variable) {\n  var budgetAsArray = budget;\n  var outputCategoryData = [];\n  var categories = [];\n  var keys = Object.keys(budget);\n  keys.forEach(function (key) {\n    categories.push(translateCategoryText(budget[key].category));\n  });\n  var allCategories = categories.filter(function (v, i, a) {\n    return a.indexOf(v) === i;\n  }).sort(function (a, b) {\n    return a - b;\n  });\n  allCategories.map(function (item) {\n    var sumCount = 0;\n    var sumCost = 0;\n    var sumVotes = 0;\n    budgetAsArray.filter(function (i) {\n      return item === translateCategoryText(i.category);\n    }).map(function (item) {\n      sumCost = sumCost + Number(item.cost);\n      if (!isNaN(item.votes)) sumVotes = sumVotes + Number(item.votes);\n      sumCount = sumCount + 1;\n    });\n    outputCategoryData.push({\n      title: \"Category: \".concat(item),\n      count: sumCount,\n      cost: sumCost,\n      votes: sumVotes\n    });\n  });\n  outputCategoryData.sort(function (a, b) {\n    return Number(a[variable]) < Number(b[variable]) ? 1 : -1;\n  });\n  outputCategoryData.splice(howMany, outputCategoryData.length - howMany); // let data = budgetAsArray.map(item => Number(item[this.props.variable]));\n\n  var data = outputCategoryData;\n  data.forEach(function (item) {\n    return item[variable] = Number(item[variable]);\n  });\n  return data;\n};","map":{"version":3,"sources":["/Users/benjamingordon/Documents/civic-alley-visualization/dat